    "test:coverage": "vitest run --coverage"
  },
  "main": "./src/server.ts",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@vitest/coverage-v8": "3.1.2",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vite-node": "3.1.2",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "express": "^5.1.0",
    "socket.io": "^4.8.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
