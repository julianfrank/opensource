{
  "name": "streambridge",
  "private": true,
  "version": "2025.4.28",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:server": "vite-node --watch ./src/server/server.ts",
    "build": "tsc && vite build",
